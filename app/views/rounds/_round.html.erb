<li id="round-<%= round.id %>">
<div class="card">
  <div class="card-block">
    <h4 class="card-title"><%= link_to "Edit Round", edit_tournament_round_path(@tournament.id) %> </h4>
    <h6 class="card-subtitle mb-2 text-muted">Strategy</h6>
    <ul class="list-group list-group-flush">
      <li class="list-group-item"><%= round.strategy %></li>
    </ul>
    <h6 class="card-subtitle mb-2 text-muted">Deployment</h6>
    <ul class="list-group list-group-flush">
      <li class="list-group-item"><%= round.deployment %></li>
    </ul>
    <h6 class="card-subtitle mb-2 text-muted">Schemes</h6>
    <ul class="list-group list-group-flush">
    <li class="list-group-item"><%= round.scheme1 %></li>
    <li class="list-group-item"><%= round.scheme2 %></li>
    <li class="list-group-item"><%= round.scheme3 %></li>
    <li class="list-group-item"><%= round.scheme4 %></li>
    <li class="list-group-item"><%= round.scheme5 %></li>
  </ul>
  </div>
</div>

</li>
